{
    "name": "cs4920-project",
    "version": "0.1.0",
    "repository": "https://github.com/mezzode/cs4920-project.git",
    "license": "AGPL-3.0-or-later",
    "scripts": {
        "build": "yarn clean:build && cd web && yarn build && cd ../server && yarn build && cd ..",
        "start:prod": "yarn build && node server/build/index.js",
        "start:db": "docker-compose up --build postgres",
        "create-db-volume": "docker volume create cs4920-postgres-db",
        "start": "docker-compose up",
        "start:build": "docker-compose up --build",
        "clean:build": "rimraf web/build && rimraf server/build"
    },
    "devDependencies": {
        "rimraf": "^2.6.2"
    }
}
